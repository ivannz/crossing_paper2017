@article{Dietrich:1997:FES:257708.257735,
	Abstract = {Geostatistical simulations often require the generation of numerous realizations of a stationary Gaussian process over a regularly meshed sample grid $\Omega$. This paper shows that for many important correlation functions in geostatistics, realizations of the associated process over $m+1$ equispaced points on a line can be produced at the cost of an initial FFT of length $2m$ with each new realization requiring an additional FFT of the same length. In particular, the paper first notes that if an $(m+1)\times(m+1) $ Toeplitz correlation matrix $R$ can be embedded in a nonnegative definite $2M\times2M$ circulant matrix $S$, exact realizations of the normal multivariate $y \sim {\cal N}(0,R)$ can be generated via FFTs of length $2M$. Theoretical results are then presented to demonstrate that for many commonly used correlation structures the minimal embedding in which $M = m$ is nonnegative definite. Extensions to simulations of stationary fields in higher dimensions are also provided and illustrated.},
	Acmid = {257735},
	Address = {Philadelphia, PA, USA},
	Author = {Dietrich, C. R. and Newsam, G. N.},
	Doi = {10.1137/S1064827592240555},
	Issn = {1064-8275},
	Issue_Date = {July 1997},
	Journal = {SIAM J. Sci. Comput.},
	Keywords = {Monte Carlo simulations, geostatistics, random fields, time series},
	Month = jul,
	Number = {4},
	Numpages = {20},
	Pages = {1088--1107},
	Publisher = {Society for Industrial and Applied Mathematics},
	Title = {Fast and Exact Simulation of Stationary Gaussian Processes Through Circulant Embedding of the Covariance Matrix},
	Url = {http://dx.doi.org/10.1137/S1064827592240555},
	Volume = {18},
	Year = {1997},
	Bdsk-Url-1 = {http://dx.doi.org/10.1137/S1064827592240555}}

@article{Perrin:1058211,
	Abstract = {In this letter, it is shown that fast and exact fractional Brownian motion (fBm) and fractional Gaussian noise (fGn) signals can be synthesized by the circulant embedding method (CEM). CEM consists in embedding the N/spl times/N covariance matrix of the stationary fGn process in a larger 2M/spl times/2M circulant matrix such that M /spl ges/N-1. CEM is exact, since second-order statistics of the generated data are those of the Gaussian fGn. CEM is fast, since the optimal case M=N-1 can be reached. Fast and exact fBm sequences can be easily recovered from fGn ones.},
